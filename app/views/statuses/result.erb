  fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));</script>
</li>
<li>  <!-- +1ボタン -->
<script src="https://apis.google.com/js/platform.js" async defer>
  {lang: 'ja'}
</script>
<div class="g-plusone" data-size="tall" data-href="http://iiname.herokuapp.com/is/<%= CGI.unescape(@name) %>" data-annotation="none"></div>
</li>
<li>  <!-- LINEで送る -->
  <span>
<script type="text/javascript" src="//media.line.me/js/line-button.js?v=20140411" ></script>
<script type="text/javascript">
new media_line_me.LineButton({"pc":false,"lang":"ja","type":"a","text":"いいね〜むでチーム名つくってみた","withUrl":true});
</script>
</span>
</li>
</ul>
